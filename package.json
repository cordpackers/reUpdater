{
  "name": "reupdater",
  "version": "0.1.0",
  "description": "An open source alternative to/replacement of Discord's Rust-programmed updater.node.",
  "private": "true",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "rimraf ./dist && npx tsc",
    "build": "rimraf ./dist && npx tsc && npm prune --omit=dev --omit=optional --omit=peer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modulocord/reUpdater.git"
  },
  "author": "Modulo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modulocord/reUpdater/issues"
  },
  "homepage": "https://github.com/modulocord/reUpdater#readme",
  "devDependencies": {
    "@types/node": "^20.5.8",
    "@types/tar-fs": "^2.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "sqlite3": "^5.1.6",
    "tar-fs": "^3.0.4"
  }
}
